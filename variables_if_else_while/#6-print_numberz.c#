#include <stdio.h>

/**
 * main - Prints single digit numbers of base 10, followed by a new line
 *
 * Return: Always 0 (Success)
 */
int main(void)
{
	int i = 0;

	while (i < 10)
	{
		putchar(i + '0');
		i++;
	}
	putchar('\n');
	return (0);
}
#include <stdio.h>

/**
 * main - Prints single digit numbers of base 10, followed by a new line
 *
 * Return: Always 0 (Success)
 */
int main(void)
{
	int i = 0;

	while (i < 10)
	{
		putchar(i + '0');
		i++;
	}
	putchar('\n');
	return (0);
}
